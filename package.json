{
  "name": "@wakecap/blockwork-ui",
  "version": "1.0.0",
  "type": "module",
  "description": "Blockwork Design System - The Ultimate Guidelines for Construction Projects",
  "keywords": [
    "react",
    "component",
    "library",
    "design-system",
    "wakecap",
    "blockwork"
  ],
  "author": "WakeCap",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wakecap/blockwork.git"
  },
  "bugs": {
    "url": "https://github.com/wakecap/blockwork/issues"
  },
  "homepage": "https://github.com/wakecap/blockwork",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist",
    "components",
    "index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:css",
    "build:css": "tailwindcss -i ./src/globals.css -o ./dist/styles.css --minify",
    "build:check": "tsc && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\" --config .prettierrc",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md,json}\" --config .prettierrc",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "ag-grid-community": "^34.1.2",
    "ag-grid-react": "^34.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.344.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.36.0",
    "@storybook/addon-a11y": "^9.1.7",
    "@storybook/addon-docs": "^9.1.7",
    "@storybook/addon-essentials": "^9.0.0-alpha.12",
    "@storybook/addon-links": "^9.1.7",
    "@storybook/addon-onboarding": "^9.1.7",
    "@storybook/react-vite": "^9.1.7",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-storybook": "^9.1.9",
    "http-server": "^14.1.1",
    "identity-obj-proxy": "^3.0.0",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^9.1.7",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^5.1.3",
    "vite-plugin-dts": "^4.5.4"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "exports": {
    "./styles.css": "./dist/styles.css",
    "./components/TopNavigator": {
      "types": "./dist/components/TopNavigator.d.ts",
      "import": "./dist/components/TopNavigator.js",
      "require": "./dist/components/TopNavigator.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "./components/TopNavigator": [
        "./dist/components/TopNavigator.d.ts"
      ]
    }
  }
}
